{
	"name": "titanium",
	"version": "7.0.0-alpha",
	"author": "TiDev, Inc. <npm@tidev.io>",
	"description": "Command line interface for building Titanium SDK apps",
	"type": "module",
	"keywords": [
		"titanium",
		"tidev",
		"mobile",
		"ios",
		"iphone",
		"android"
	],
	"homepage": "https://titaniumsdk.com",
	"bugs": "https://github.com/tidev/titanium-cli/issues",
	"repository": "https://github.com/tidev/titanium-cli",
	"preferGlobal": true,
	"dependencies": {
		"@xmldom/xmldom": "0.8.10",
		"chalk": "5.3.0",
		"commander": "11.0.0",
		"execa": "8.0.1",
		"fs-extra": "11.1.1",
		"pretty-bytes": "6.1.1",
		"prompts": "2.4.2",
		"semver": "7.5.4",
		"undici": "5.24.0",
		"which": "4.0.0",
		"wrap-ansi": "8.1.0",
		"xpath": "0.0.33",
		"yauzl": "2.10.0"
	},
	"devDependencies": {
		"@vitest/coverage-v8": "0.34.4",
		"esbuild": "0.19.3",
		"eslint": "8.49.0",
		"eslint-plugin-promise": "6.1.1",
		"eslint-plugin-security": "1.7.1",
		"vitest": "0.34.4"
	},
	"license": "Apache-2.0",
	"bin": {
		"ti": "./bin/ti.js",
		"titanium": "./bin/ti.js"
	},
	"files": [
		"./dist"
	],
	"scripts": {
		"build": "node build.js",
		"coverage": "vitest run --coverage",
		"lint": "eslint --cache \"src/**/*.js\"",
		"prepack": "node build.js",
		"test": "vitest"
	},
	"engines": {
		"node": ">=18"
	},
	"packageManager": "pnpm@8.7.4"
}
