{
	"name": "titanium",
	"version": "7.0.0-alpha",
	"author": "TiDev, Inc. <npm@tidev.io>",
	"description": "Command line interface for building Titanium SDK apps",
	"type": "module",
	"about": {
		"name": "Titanium Command-Line Interface",
		"author": "TiDev",
		"copyright": "Copyright TiDev, Inc. 4/7/2022-Present. All Rights Reserved.",
		"id": "com.appcelerator.titanium.cli"
	},
	"keywords": [
		"titanium",
		"tidev",
		"mobile",
		"ios",
		"iphone",
		"android"
	],
	"homepage": "https://github.com/tidev/titanium",
	"bugs": "https://github.com/tidev/titanium/issues",
	"repository": "https://github.com/tidev/titanium",
	"preferGlobal": true,
	"dependencies": {
		"@clack/prompts": "0.7.0",
		"@xmldom/xmldom": "0.8.10",
		"chalk": "5.3.0",
		"commander": "11.0.0",
		"execa": "8.0.1",
		"fs-extra": "11.1.1",
		"humanize": "0.0.9",
		"semver": "7.5.4",
		"sprintf-js": "1.1.2",
		"tmp": "0.2.1",
		"undici": "5.23.0",
		"which": "3.0.1",
		"wrap-ansi": "8.1.0",
		"xpath": "0.0.33",
		"yauzl": "2.10.0"
	},
	"devDependencies": {
		"@commitlint/cli": "17.7.1",
		"@commitlint/config-conventional": "17.7.0",
		"danger": "11.2.8",
		"eslint": "8.47.0",
		"eslint-config-axway": "8.0.0",
		"eslint-plugin-mocha": "10.1.0",
		"husky": "8.0.3",
		"lint-staged": "14.0.1",
		"mocha": "10.2.0",
		"nyc": "15.1.0",
		"should": "13.2.3"
	},
	"license": "Apache-2.0",
	"bin": {
		"ti": "./bin/ti.js",
		"titanium": "./bin/ti.js"
	},
	"scripts": {
		"lint": "eslint .",
		"test": "npm run lint && npm run unit-test",
		"unit-test": "nyc mocha \"tests/commands/test-*.js\"",
		"prepare": "husky install"
	},
	"mocha": {
		"timeout": 3000,
		"check-leaks": true,
		"globals": [
			"Hyperloop",
			"HyperloopObject"
		]
	},
	"nyc": {
		"check-coverage": false,
		"lines": 55,
		"statements": 55,
		"functions": 49,
		"branches": 44,
		"exclude": [
			"tests/**/*.js",
			"locales/**/*.js"
		],
		"reporter": [
			"cobertura",
			"text"
		]
	},
	"engines": {
		"node": ">=18"
	},
	"lint-staged": {
		"*.js": "eslint"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	}
}
